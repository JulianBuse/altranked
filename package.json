{
	"name": "altranked",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"web": "vite --port 3000 --clearScreen false",
		"api": "wrangler dev --port 8787",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"generate": "drizzle-kit generate:sqlite --schema=./api/db/schema.ts --out=./drizzle",
		"up:dev": "wrangler d1 migrations apply altranked --local",
		"up:prod": "wrangler d1 migrations apply altranked",
		"deploy:api": "wrangler deploy",
		"deploy:web": "wrangler pages deploy dist --project-name altranked",
		"deploy": "npm run build && npm run deploy:api && npm run up:prod && npm run deploy:web"
	},
	"dependencies": {
		"@radix-ui/react-progress": "^1.0.3",
		"@trpc-swr/client": "^1.0.2",
		"@trpc/client": "^10.38.5",
		"@trpc/server": "^10.38.5",
		"bcryptjs": "^2.4.3",
		"drizzle-orm": "^0.28.6",
		"fast-deep-equal": "^3.1.3",
		"framer-motion": "^10.16.4",
		"hono": "^3.7.3",
		"immer": "^10.0.3",
		"moment": "^2.29.4",
		"nanoid": "^5.0.1",
		"react": "^18.2.0",
		"react-countup": "^6.4.2",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"react-use-websocket": "^4.5.0",
		"swr": "^2.2.4",
		"use-immer": "^0.9.0",
		"usehooks-ts": "^2.9.1",
		"wouter": "^2.12.0-beta.1",
		"zod": "^3.22.3",
		"zustand": "^4.4.2"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230419.0",
		"@tailwindcss/forms": "^0.5.6",
		"@types/bcryptjs": "^2.4.4",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.4",
		"vite": "^4.4.5",
		"vite-tsconfig-paths": "^4.2.1",
		"wrangler": "^3.0.0"
	}
}
